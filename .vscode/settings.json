{
  // Configure glob patterns for excluding files and folders.
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/LICENSE": true,
    "**/.gitattributes": true,
    "**/.gitignore": false,
    "**/*.meta": true,
    "**/*.*.meta": true,
    "**/*.unity": true,
    "**/*.unityproj": true,
    "**/*.mat": true,
    "**/*.fbx": true,
    "**/*.FBX": true,
    "**/*.tga": true,
    "**/*.cubemap": true,
    "**/*.prefab": true,
    "**/*.csproj": true,
    "**/*.sln": true,
    "**/Library": true,
    "**/ProjectSettings": true,
    "**/Temp": true,
    "**/Logs": true,
    "**/obj": true,
    "**/Packages": true
  }
}
